{
  "name": "test_cafe",
  "version": "1.0.0",
  "description": "automated testing with test cafe",
  "main": "index.js",
  "scripts": {
    "test": "testcafe chrome:headless ./tests",
    "test-chrome": "testcafe chrome ./tests -s takeOnFails=true --disable-page-caching",
    "test-edge": "testcafe edge ./tests",
    "test-mobile": "testcafe chrome:emulation:device=iphone X ./tests"
  },
